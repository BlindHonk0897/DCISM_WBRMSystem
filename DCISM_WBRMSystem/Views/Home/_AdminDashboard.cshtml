<div class="row">
    <!--[ daily sales section ] start-->
    <div class="col-md-6 col-xl-4 step1">
        <div class="card daily-sales">
            <div class="card-block">
                <h6 class="mb-4">No. of Available Items</h6>
                <div class="row d-flex align-items-center">
                    <div class="col-9">
                        <h3 class="f-w-300 d-flex align-items-center m-b-0" id="Available_Items"></h3>
                    </div>

                    <div class="col-3 text-right">
                        <p class="m-b-0"></p>
                    </div>
                </div>
                <div class="progress m-t-30" style="height: 7px;">
                    <div class="progress-bar progress-c-theme" role="progressbar" style="width: 50%;" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
        </div>
    </div>
    <!--[ daily sales section ] end-->
    <!--[ Monthly  sales section ] starts-->
    <div class="col-md-6 col-xl-4 ">
        <div class="card Monthly-sales">
            <div class="card-block">
                <h6 class="mb-4">Borrowed Items</h6>
                <div class="row d-flex align-items-center">
                    <div class="col-9">
                        <h3 class="f-w-300 d-flex align-items-center  m-b-0" id="Borrowed_Items"></h3>
                    </div>
                    <div class="col-3 text-right">
                        <p class="m-b-0"></p>
                    </div>
                </div>
                <div class="progress m-t-30" style="height: 7px;">
                    <div class="progress-bar progress-c-theme2" role="progressbar" style="width: 35%;" aria-valuenow="35" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
        </div>
    </div>
    <!--[ Monthly  sales section ] end-->
    <!--[ year  sales section ] starts-->
    <div class="col-md-12 col-xl-4">
        <div class="card yearly-sales">
            <div class="card-block">
                <h6 class="mb-4">Damaged Items</h6>
                <div class="row d-flex align-items-center">
                    <div class="col-9">
                        <h3 class="f-w-300 d-flex align-items-center  m-b-0" id="Damaged_Items"></h3>
                    </div>
                    <div class="col-3 text-right">
                        <p class="m-b-0"></p>
                    </div>
                </div>
                <div class="progress m-t-30" style="height: 7px;">
                    <div class="progress-bar progress-c-red" role="progressbar" style="width: 30%;" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
        </div>
    </div>
    <!--[ year  sales section ] end-->
    <!--[ Recent Users ] start-->
    <div class="col-xl-9 col-md-6 step2">
        <div class="card Recent-Users">
            <div class="card-header">
                <h5>No. of stocks per Facility</h5>
            </div>
            <div class="card-block px-0 py-3 ">
                @*<div id="chartContainer" style="height: 300px; width: 100%;"></div>*@
                <canvas id="chartContainer" style="height: 300px; width: 100%;"></canvas>
            </div>
        </div>
    </div>
    <!--[ Recent Users ] end-->
    <!-- [ statistics year chart ] start -->
    <div class="col-xl-3 col-md-6">
        <div class="card">
            <div class="card-block border-bottom">
                <div class="row d-flex align-items-center">
                    <div class="col-auto">
                        <i class="feather icon-map-pin f-30 text-c-green"></i>
                    </div>
                    <div class="col">
                        <h3 class="f-w-300">LB 448</h3>
                        <span class="d-block text-uppercase">Most Reserved Facility</span>
                    </div>
                </div>
            </div>
            <div class="card-block">
                <div class="row d-flex align-items-center">
                    <div class="col-auto">
                        <i class="fa fa-users f-30 text-c-blue"></i>
                    </div>
                    <div class="col">
                        <h3 class="f-w-300" id="Registered_Users"></h3>
                        <span class="d-block text-uppercase">Registered Users</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- [ statistics year chart ] end -->

</div>
